        <!-- Main Content -->
        <main class="py-6 md:py-12 px-4 md:px-6">
          <section class="text-gray-900 dark:text-gray-200">
            <div class="taglines">
             <span><h1 class="text-4xl">Welcome to Zayka</h1>:Where Taste Meets Inspiration</span>
              <p class="text-3xl mb-6 text-teal-500 font-extrabold">Explore delicious recipes and cooking tips,here are some quick Categories.</p>
            </div>
            
            <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6 p-6">
              <% categories.forEach(function(category, index) { %>
                <% if (index === categories.length - 1) { %>
                  <!-- Last card with "View All" functionality -->
                  <div class="max-w-sm rounded-lg overflow-hidden shadow-lg bg-white hover:shadow-2xl hover:scale-105 hover:border-blue-600 border border-gray-200 transform transition-all duration-300 ease-in-out">
                    <a href="/view-all">
                      <img class="w-full" src="https://res.cloudinary.com/dvmqxb8kd/image/upload/v1726026502/my-recipes-folder/uivk0exodcooerjq1myh.jpg" alt="View All Recipes">
                      <div class="px-6 py-4">
                        <h2 class="text-2xl font-bold text-blue-600">View Details</h2>
                        <p class="text-gray-700 text-base">Browse amazing details of each category!</p>
                      </div>
                    </a>
                  </div>
                <% } else { %>
                  <!-- Regular category card -->
                  <div class="max-w-sm rounded-lg overflow-hidden shadow-lg bg-white hover:shadow-2xl hover:scale-105 hover:border-green-600 border border-gray-200 transform transition-all duration-300 ease-in-out">
                    <a href="/category/<%= category.name %>">
                      <img class="w-full" src="<%= category.image %>" alt="<%= category.name %> recipe">
                      <div class="px-6 py-4">
                        <h2 class="text-xl font-bold mb-2 text-green-600"><%= category.name %></h2>
                        <p class="text-gray-700 text-base">Enjoy delicious <%= category.name %> food with our easy-to-follow recipes.</p>
                      </div>
                      <div class="px-6 pt-4 pb-2">
                        <span class="inline-block bg-green-200 rounded-full px-3 py-1 text-sm font-semibold text-green-700 mr-2 mb-2">#<%= category.name %></span>
                        <span class="inline-block bg-green-200 rounded-full px-3 py-1 text-sm font-semibold text-green-700 mr-2 mb-2">#Cuisine</span>
                      </div>
                    </a>
                  </div>
                <% } %>
              <% }); %>
            </div>
            
            
            
            
            
          </section>
